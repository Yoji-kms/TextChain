        app:layout_constraintVertical_chainStyle="spread"
        tools:text="M" />

    <TextView
        android:id="@+id/endColumnLtr2TxtViewId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        android:textSize="20sp"

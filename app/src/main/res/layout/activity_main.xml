    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView android:id="@+id/calcSmallTxtViewId"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:gravity="center_vertical|end"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView android:id="@+id/calcMainTxtView"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:gravity="center_vertical|end"
        android:textSize="60sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/calcSmallTxtViewId" />

    <LinearLayout android:id="@+id/actionRowId"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_toStartOf="@id/actionColumnId"
        android:orientation="horizontal"
        app:layout_constraintEnd_toStartOf="@id/guideline75"
        app:layout_constraintStart_toStartOf="@id/guideline5"
        app:layout_constraintTop_toBottomOf="@id/topRowLtr1TxtViewId">

        <Button
            android:id="@+id/clearBtnId"
            style="@android:style/Widget.DeviceDefault.Button"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:backgroundTint="@color/colorActionBtn"
            android:text="@string/clear"
            android:textSize="40sp"
            tools:ignore="NestedWeights" />

        <Button
            android:id="@+id/signBtnId"
            style="@android:style/Widget.DeviceDefault.Button"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:backgroundTint="@color/colorActionBtn"
            android:text="@string/sign"
            android:textSize="40sp" />

        <Button
            android:id="@+id/percentBtnId"
            style="@android:style/Widget.DeviceDefault.Button"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:backgroundTint="@color/colorActionBtn"
            android:text="@string/percent"
            android:textSize="40sp" />
    </LinearLayout>

    <LinearLayout android:id="@+id/topNumRowId"
